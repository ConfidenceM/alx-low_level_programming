#include <stdio.h>
#include <ctype.h>
/**
 * main - prints all  the alphabet on a new linw
 * Return: return 0
 */
int main(void)
{
int letter;

for (letter = 'a'; letter <= 'z'; letter++)
{
letter = tolower(letter);
putchar(letter);
if (letter == 'z')
{
letter = 'a'	
for (; letter <= 'Z'; letter++)							
{
	putchar(letter);
}
break;
}
}
putchar('\n');
return (0);
}
